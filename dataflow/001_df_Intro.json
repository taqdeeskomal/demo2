{
	"name": "001_df_Intro",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Df_01_csv",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "01_df_intro",
						"type": "DatasetReference"
					},
					"name": "sink"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable30",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          OrderDate as date,",
				"          ProductKey as short,",
				"          Country as string,",
				"          EnglishProductName as string,",
				"          SalesAmount as double,",
				"          UnitPrice as double,",
				"          OrderQuantity as boolean,",
				"          TaxAmt as double,",
				"          TotalProductCost as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 select(mapColumn(",
				"          OrderDate,",
				"          ProductKey,",
				"          Country,",
				"          SalesAmount,",
				"          UnitPrice,",
				"          OrderQuantity,",
				"          TaxAmt,",
				"          TotalProductCost",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(Country!='Germany') ~> filter1",
				"filter1 derive(YEAR = year(OrderDate)) ~> derivedColumn1",
				"derivedColumn1 split(Country=='France',",
				"     disjoint: false) ~> split1@(France, others)",
				"split1@France sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink",
				"split1@others sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}