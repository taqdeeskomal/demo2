{
	"name": "Countrywisedata",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"CountryFile": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().CountryFile",
					"type": "Expression"
				},
				"folderPath": "output/Countries",
				"fileSystem": "adf-learning-material"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "OrderDate",
				"type": "String"
			},
			{
				"name": "ProductKey",
				"type": "String"
			},
			{
				"name": "TaxAmt",
				"type": "String"
			}
		]
	}
}